<Window x:Class="MyMvvmApp.Views.ControlPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyMvvmApp.Views"
        mc:Ignorable="d"
        Title="ControlPanel" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Center" Height="100" Margin="0,149,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="555,122,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-33.696"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Margin="433,122,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="469,256,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="527,190,0,0" VerticalAlignment="Top"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="90,210,-90,-210">
            <StackPanel>
                <TabControl Height="61">
                    <TabItem Header="TabItem" RenderTransformOrigin="0.5,0.5">
                        <TabItem.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="0"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </TabItem.RenderTransform>
                        <Grid Background="#FFE5E5E5"/>
                    </TabItem>
                    <TabItem Header="TabItem">
                        <Grid Background="#FFE5E5E5"/>
                    </TabItem>
                </TabControl>
                <TextBlock TextWrapping="Wrap" Text="TextBlock" Height="60"/>
            </StackPanel>
        </Border>
        <ComboBox HorizontalAlignment="Left" Margin="90,80,0,0" VerticalAlignment="Top" Width="132" Height="40"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="184,169,267,172" RenderTransformOrigin="0.5,0.5">
            <ListBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="57.492"/>
                    <TranslateTransform/>
                </TransformGroup>
            </ListBox.RenderTransform>
        </ListBox>
        <RadioButton Content="RadioButton" HorizontalAlignment="Left" Margin="433,158,0,0" VerticalAlignment="Top"/>
        <Rectangle HorizontalAlignment="Left" Height="100" Margin="283,20,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
        <TextBox HorizontalAlignment="Left" Margin="499,61,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="35.72"/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>

    </Grid>
</Window>
